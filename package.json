{
  "name": "@magda/minion-format",
  "description": "MAGDA Layering Minion",
  "version": "0.0.57-0",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "build": "yarn run compile",
    "compile": "tsc -b",
    "watch": "tsc -b --watch",
    "start": "node dist/index.js",
    "dev": "run-typescript-in-nodemon src/index.ts",
    "docker-build-local": "create-docker-context-for-node-component --build --push --tag auto --local",
    "docker-build-prod": "create-docker-context-for-node-component --build --push --tag auto",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register \"src/test/**/*.spec.ts\"",
    "helm-lint": "helm lint deploy/magda-minion-format -f deploy/test-deploy.yaml",
    "retag-and-push": "retag-and-push"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@magda/scripts": "^0.0.57-0",
    "@types/chai": "^4.2.8",
    "@types/ftp": "^0.3.29",
    "@types/lodash": "^4.14.88",
    "@types/lru-cache": "4.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^7.0.1",
    "@types/request": "^2.48.1",
    "@types/sinon": "^7.5.1",
    "@types/urijs": "^1.15.34",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "nock": "^11.7.2",
    "jsverify": "^0.8.2",
    "sinon": "^8.1.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@magda/minion-sdk": "^0.0.57-0",
    "@magda/registry-aspects": "^0.0.57-0",
    "@magda/utils": "^0.0.57-0",
    "@types/escape-string-regexp": "^0.0.32",
    "@types/lodash": "^4.14.88",
    "escape-string-regexp": "^1.0.5",
    "ftp": "^0.3.10",
    "load-json-file": "^4.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "4.0.2",
    "mime-types": "^1.0.0",
    "request": "^2.88.0",
    "urijs": "^1.19.6"
  },
  "config": {
    "registryUrl": "http://localhost:6101/v0",
    "docker": {
      "name": "data61/magda-minion-format",
      "include": "node_modules dist Dockerfile synonyms.json package.json"
    },
    "jwtSecret": "squirrel",
    "userId": "00000000-0000-4000-8000-000000000000"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "minion": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
